 "html": "<style>\n    @page {\n        size: 100mm 50mm; /* Set the page size to match the label dimensions */\n        margin: 0; /* Remove default margins */\n    }\n\n    .label-table {\n        width: 100mm !important; /* Fixed width */\n        height: 50mm !important; /* Fixed height */\n        font-size: 10px;\n        border-collapse: collapse;\n        text-align: center;\n    }\n\n    .label-table td {\n        border-bottom: 1px solid black;\n    }\n\n    .label-table tr:first-child td {\n        border-top: none; /* Remove top border */\n    }\n\n    .label-table tr:last-child td {\n        border-bottom: none !important; /* Remove bottom border */\n    }\n\n    .label-table td:first-child {\n        border-right: 1px solid black;\n        font-weight: bold;\n    }\n\n    /* Barcode Row Styling */\n    .barcode-row td {\n        border-bottom: none !important; /* Remove bottom border for barcode row */\n        text-align: center;\n        border-right: none !important;\n    }\n\n    /* Barcode Image Centering */\n    .barcode-row img {\n        display: block;\n        margin: auto;\n        min-width: 50%;\n        max-width: 80%; /* Ensures barcode is not too wide */\n        height: auto;\n        max-height: 10mm;\n    }\n\n    /* Ensure each label prints on a new page */\n    .label-container {\n        page-break-after: always;\n    }\n</style>\n\n<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Product Label</title>\n</head>\n<body>\n\n    <div class=\"label-container\">\n        <table class=\"label-table\">\n            <tr>\n                <td>PRODUCT CODE</td>\n                <td>{{ frappe.db.get_value(\"Item\", doc.item_name, \"item_code\") or ' ' }}</td>\n            </tr>\n            <tr>\n                <td>PRODUCT NAME</td>\n                <td>{{ doc.item_name or ' ' }}</td>\n            </tr>\n            <tr>\n                <td>MASTER CARTON NO.</td>\n                <td>{{ doc.master_box_serial or 'N/A' }}</td>\n            </tr>\n            <tr>\n                <td>NO. OF COILS</td>\n                <td>{{ doc.serial_nos | length }}</td>\n            </tr>\n            <tr class=\"barcode-row\">\n                <td colspan=\"2\">\n                    <img src=\"https://barcode.tec-it.com/barcode.ashx?data={{ doc.master_box_serial }}&code=Code128&dpi=300&imagetype=png\" alt=\"Barcode\">\n                </td>\n            </tr>\n        </table>\n    </div>\n\n</body>\n</html>\n",
