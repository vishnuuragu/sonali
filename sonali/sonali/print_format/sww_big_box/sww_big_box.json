 "font_size": 14,
 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Sticker Label</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            font-size: 11px;\n        }\n        .flex-container {\n            display: flex;\n            align-items: flex-start; /* Align both elements at the top */\n            gap: 10px; /* Space between sticker and box table */\n        }\n        .sticker {\n            width: 378px;\n            height: 189px;\n            padding: 5px;\n            text-align: left;\n            background: white;\n            margin-bottom: 15px;\n            display: flex;\n            flex-direction: column;\n            justify-content: space-between;\n            overflow: hidden;\n        }\n        .sticker table {\n            width: 100%;\n            border-collapse: collapse;\n        }\n        .sticker table tr:first-child td {\n            border-top: none; /* Remove the top border for the first row */\n        }\n        .sticker table td {\n            padding: 2px;\n            font-size: 10px;\n            border-top: 1px solid black; \n            border-bottom: 1px solid black;\n        }\n        .sticker table td:first-child {\n            width: 50%;\n            font-weight: bold;\n            border-right: 1px solid black;\n            text-align: left;\n            padding-left: 5px;\n        }\n        .sticker table td:last-child {\n            text-align: right;\n            padding-right: 5px;\n        }\n        .barcode {\n            text-align: center;\n            margin-top: 5px;\n            width: 100%;\n            height: 40px;\n            overflow: hidden;\n        }\n        .barcode img {\n            width: 60%;\n            height: auto;\n            object-fit: contain;\n        }\n        .box-table {\n            width: 200px;\n            border-collapse: collapse;\n        }\n        .box-table th, .box-table td {\n            border: 1px solid #000;\n            padding: 3px;\n            text-align: center;\n            font-size: 10px;\n        }\n        .box-table th {\n            font-weight: bold;\n        }\n    </style>\n</head>\n<body>\n\n{% for row in doc.serial_nos %}\n<div class=\"flex-container\">\n    <!-- Sticker Label -->\n    <div class=\"sticker\">\n        <table>\n            <tr>\n                <td>SIZE (MM)</td>\n                <td>{{ doc.size_mm }}</td>\n            </tr>\n            <tr>\n                <td>LENGTH (MTRS.)</td>\n                <td>{{ row.length }}</td>\n            </tr>\n            <tr>\n                <td>WEIGHT (KGS)</td>\n                <td>{{ doc.weight }}</td>\n            </tr>\n            <tr>\n                <td>BOX NUMBER</td>\n                <td>{{ row.serial_no }}</td>\n            </tr>\n            <tr>\n                <td>MFG. DATE</td>\n                <td>{{ frappe.utils.formatdate(doc.mfg_date, \"dd/mm/yyyy\") if doc.mfg_date else 'DD/MM/YYYY' }}</td>\n            </tr>\n        </table>\n        <div class=\"barcode\">\n            <img src=\"https://barcode.tec-it.com/barcode.ashx?data={{ row.serial_no }}&code=Code128&dpi=96\" alt=\"Barcode\">\n        </div>\n    </div>\n\n    <!-- Box Table -->\n    <table class=\"box-table\">\n        <thead>\n            <tr>\n                <th>Box No.</th>\n                <th>Size (MM)</th>\n                <th>Weight</th>\n                <th>Length</th>\n            </tr>\n        </thead>\n        <tbody>\n            {% for row in doc.serial_nos %}\n                <tr>\n                    <td>{{ row.serial_no }}</td>\n                    <td>{{ doc.size_mm }}</td>\n                    <td>{{ row.weight }}</td>\n                    <td>{{ row.length }}</td>\n                </tr>\n            {% endfor %}\n        </tbody>\n    </table>\n</div>\n{% endfor %}\n\n</body>\n</html>\n",
